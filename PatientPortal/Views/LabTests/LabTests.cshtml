@{
    ViewData["Title"] = "Lab Tests";
}

@model PatientPortal.Models.LabTests
@{

}

<div class="text-center">
    <h1>
    Welcome To Labtests Results.
    </h1>
</div>
   
 <div class="container">
    <table class="table table hover">
        <thead>
        <tr>
            <th>Test Id</th>
             <th>Lab Name</th>
            <th>Results</th>
           <th>Test Date</th>
        </tr>
        </thead>

    <tbody>
        <tr>
            <td>@Model.LabTestId</td>
            <td>@Model.LabTestname</td>
            <td>@Model.LabResults</td>
            <td>@Model.TestDate</td>
        </tr>

    </tbody>
     
     </table>
 </div>